    "def Parity(x, y, z):\n",
    "    # Ensure inputs are treated as 32-bit unsigned integers\n",
    "    x, y, z = np.uint32(x), np.uint32(y), np.uint32(z)\n",
    "    # Return the bitwise XOR of the three inputs\n",
    "    return x ^ y ^ z"
   ]
